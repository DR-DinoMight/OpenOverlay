{
    "name": "redbeed/openoverlay",
    "description": ":package_description",
    "license": "license",
    "authors": [
        {
            "name": "author name",
            "email": "author email",
            "homepage": "author homepage"
        }
    ],
    "homepage": "https://github.com/redbeed/openoverlay",
    "keywords": ["Laravel", "OpenOverlay"],
    "require": {
        "illuminate/support": "~7|~8",
        "socialiteproviders/twitch": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "repositories": {
        "socialiteproviders/twitch": {
            "url": "https://github.com/redbeed/TwitchClientCredentials.git",
            "type": "git"
        }
    },
    "autoload": {
        "psr-4": {
            "Redbeed\\OpenOverlay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Redbeed\\OpenOverlay\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Redbeed\\OpenOverlay\\OpenOverlayServiceProvider"
            ],
            "aliases": {
                "OpenOverlay": "Redbeed\\OpenOverlay\\Facades\\OpenOverlay"
            }
        }
    }
}
