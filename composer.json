{
    "name": "redbeed/openoverlay",
    "description": "Receive events from Twitch-EventSub Api with Laravel.",
    "license": "MIT",
    "authors": [
        {
            "name": "Chris Woelk",
            "email": "hello@redbeed.com",
            "homepage": "https://www.redbeed.com"
        }
    ],
    "homepage": "https://github.com/redbeed/openoverlay",
    "keywords": ["Laravel", "OpenOverlay", "twitch", "Eventsub"],
    "require": {
        "illuminate/support": "~7|~8",
        "guzzlehttp/guzzle": "^7.2",
        "ext-json": "*",
        "socialiteproviders/twitch": "^5.3",
        "react/socket": "^1.6",
        "ratchet/pawl": "^0.3.5",
        "spatie/url": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6",
        "nunomaduro/phpinsights": "^1.14"
    },
    "autoload": {
        "psr-4": {
            "Redbeed\\OpenOverlay\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Redbeed\\OpenOverlay\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Redbeed\\OpenOverlay\\OpenOverlayServiceProvider"
            ],
            "aliases": {
                "OpenOverlay": "Redbeed\\OpenOverlay\\Facades\\OpenOverlay"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
